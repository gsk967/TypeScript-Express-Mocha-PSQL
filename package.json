{
	"name": "typescript-express-psql",
	"version": "1.0.0",
	"description": "TypeScript and Express and PSQL ",
	"main": "server.js",
	"scripts": {
		"start": "tsc",
		"dev": "concurrently -k -p \"[{name}]\" -n \"Running server... \" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
		"test": "concurrently -k -p \"[{name}]\" -n \"Running testing server... \" -c \"yellow.bold,cyan.bold,green.bold\" \"NODE_ENV=testing npm run prescript\" \"mocha --require ts-node/register test/*.ts\" ",
		"watch-ts": "tsc -w",
		"watch-node": "nodemon dist/server.js",
		"tslint": "tslint -c tslint.json -p tsconfig.json",
		"tslint-fix": "tslint --fix -p tsconfig.json -t verbose",
		"prescript": "ts-node scripts/prescript",
		"apidoc": "tsc && apidoc -i ./dist -o apidocs && serve apidocs"
	},
	"apidoc": {
		"title": "TypeScript and Express and PSQL",
		"url": "http://localhost:3000"
	},
	"keywords": [
		"typescript",
		"and",
		"express",
		"and",
		"psql"
	],
	"author": "saikumar <gskiiit967@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/async": "^2.4.2",
		"@types/bluebird": "^3.5.26",
		"@types/body-parser": "^1.17.0",
		"@types/chai": "^4.1.7",
		"@types/chai-http": "^4.2.0",
		"@types/config": "^0.0.34",
		"@types/cors": "^2.8.5",
		"@types/dotenv": "^6.1.1",
		"@types/express": "^4.16.1",
		"@types/joi": "^14.3.3",
		"@types/jsonwebtoken": "^8.3.2",
		"@types/mocha": "^5.2.6",
		"@types/pg": "^7.4.14",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"concurrently": "^4.1.0",
		"mocha": "^6.1.4",
		"serve": "^11.0.0",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5"
	},
	"dependencies": {
		"apidoc": "^0.17.7",
		"async": "^2.6.2",
		"bluebird": "^3.5.4",
		"body-parser": "^1.19.0",
		"config": "^3.1.0",
		"cors": "^2.8.5",
		"dotenv": "^8.0.0",
		"express": "^4.16.4",
		"glob": "^7.1.4",
		"helmet": "^3.18.0",
		"joi": "^14.3.1",
		"joi-objectid": "^2.0.0",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.24.0",
		"morgan": "^1.9.1",
		"path": "^0.12.7",
		"pg": "^7.10.0",
		"request": "^2.88.0",
		"ts-node": "^8.1.0",
		"uuid": "^3.3.2"
	}
}
